    "editor.defaultColorDecorators": "auto",
    "editor.occurrencesHighlight": "off",
    "editor.language.colorizedBracketPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.cursorBlinking": "blink",
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on"
    },
    "editor.tabCompletion": "on",
    "workbench.activityBar.location": "top",
    "editor.renderLineHighlight": "all",
    "scm.diffDecorations": "all",
    "scm.diffDecorationsGutterPattern": {
        "added": true
    },
    "git.useEditorAsCommitInput": true,
    "workbench.colorTheme": "Solarized Dark",
    "workbench.preferredDarkColorTheme": "Solarized Dark",
    "editor.fontSize": 14.5,
    "telemetry.telemetryLevel": "off",
    "markdown.preview.breaks": true,
    "workbench.statusBar.visible": false,
    "git.openRepositoryInParentFolders": "always",
    "[markdown]": {
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": false,
        },
        "editor.fontSize": 14.5,
    },
    "markdown.preview.fontSize": 14.5,
    "breadcrumbs.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.stickyScroll.enabled": false,
    "editor.scrollbar.vertical": "visible",
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.minimap.showSlider": "always",
    "editor.scrollBeyondLastLine": true,
    "output.smartScroll.enabled": true,
    "markdown.styles": [
        ".vscode/css/styles.css"
    ],
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "[typst]": {
        "editor.codeLens": false
    },
    "tinymist.formatterMode": "typstyle",
    "tinymist.completion.postfix": false,
    "tinymist.completion.postfixUfcs": false,
    "tinymist.completion.postfixUfcsLeft": false,
    "tinymist.completion.postfixUfcsRight": false,
    "tinymist.preview.partialRendering": false,
    "editor.snippetSuggestions": "top",
    "editor.foldingStrategy": "indentation",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "string.quoted.other.typst"
                ],
                "settings": {
                    "foreground": "#8534ff"
                }
            },
            {
                "scope": [
                    "string.other.link.title.typst"
                ],
                "settings": {
                    "foreground": "#008690"
                }
            },
            {
                "scope": [
                    "string",
                ],
                "settings": {
                    "foreground": "#051aff"
                }
            },
            {
                "scope": [
                    "string.quoted.double.typst" // not work ...
                ],
                "settings": {
                    "foreground": "#c10047"
                }
            },
            {
                "scope": [
                    "entity.name.function"
                ],
                "settings": {
                    "foreground": "#d28c00"
                }
            },
            {
                "scope": [
                    "variable.other"
                ],
                "settings": {
                    "foreground": "#a8003e"
                }
            },
            {
                "scope": [
                    "markup.heading"
                ],
                "settings": {
                    "foreground": "#676767"
                }
            },
            {
                "scope": [
                    "markup.content.brace.typst"
                ],
                "settings": {
                    "foreground": "#bd0000"
                }
            }
        ]
    },
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "text.math": "#2c8a00",
            "string.math": "#a84100",
            "operator.math": "#2f00ff"
        },
    },
    "editor.lineHeight": 0,
    "scm.inputFontSize": 13,
    "tinymist.rootPath": "/home/user/Folder/Write/note-math",
    "search.useIgnoreFiles": false // change to your system's root path
}