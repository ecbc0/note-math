    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "string.quoted.other.typst"
                ],
                "settings": {
                    "foreground": "#8534ff"
                }
            },
            {
                "scope": [
                    "string.other.link.title.typst"
                ],
                "settings": {
                    "foreground": "#008690"
                }
            },
            {
                "scope": [
                    "string",
                ],
                "settings": {
                    "foreground": "#051aff"
                }
            },
            {
                "scope": [
                    "string.quoted.double.typst" // not work ...
                ],
                "settings": {
                    "foreground": "#c10047"
                }
            },
            {
                "scope": [
                    "entity.name.function"
                ],
                "settings": {
                    "foreground": "#d28c00"
                }
            },
            {
                "scope": [
                    "variable.other"
                ],
                "settings": {
                    "foreground": "#a8003e"
                }
            },
            {
                "scope": [
                    "markup.heading"
                ],
                "settings": {
                    "foreground": "#676767"
                }
            },
            {
                "scope": [
                    "markup.content.brace.typst"
                ],
                "settings": {
                    "foreground": "#bd0000"
                }
            }
        ]
    },
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "text.math": "#2c8a00",
            "string.math": "#a84100",
            "operator.math": "#2f00ff"
        },
    },
    "editor.defaultColorDecorators": "auto",
    "editor.occurrencesHighlight": "off",
    "editor.language.colorizedBracketPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "search.useGlobalIgnoreFiles": false,
    "git-log--graph.hide-sidebar-buttons": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "github.gitAuthentication": true,
    "github.showAvatar": false,
    "editor.stickyScroll.defaultModel": "outlineModel"
}