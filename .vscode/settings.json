{
    "editor.fontFamily": "'Source Code Pro', 'Noto Sans Math', 'Sarasa Gothic SC', Monospace",
    "editor.fontSize": 14.5,
    "[typst]": {
        "editor.codeLens": false
    },
    "tinymist.formatterMode": "typstyle",
    "tinymist.completion.postfix": false,
    "tinymist.completion.postfixUfcs": false,
    "tinymist.completion.postfixUfcsLeft": false,
    "tinymist.completion.postfixUfcsRight": false,
    "tinymist.preview.partialRendering": false,
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on"
    },
    "editor.tabCompletion": "on",
    "editor.snippetSuggestions": "top",
    "editor.foldingStrategy": "indentation",
    "editor.lineHeight": 0,
    "scm.inputFontSize": 13,
    "search.useIgnoreFiles": true,
    "debug.console.fontSize": 13,
    "terminal.integrated.fontSize": 13,
    "chat.editor.fontSize": 13,
    "editor.wordWrap": "on",
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.glyphMargin": false,
    "editor.cursorBlinking": "blink",
    "workbench.activityBar.location": "top",
    "editor.renderLineHighlight": "all",
    "scm.diffDecorations": "all",
    "scm.diffDecorationsGutterPattern": {
        "added": true
    },
    "git.useEditorAsCommitInput": true,
    "workbench.colorTheme": "Solarized Dark",
    "workbench.preferredDarkColorTheme": "Solarized Dark",
    "telemetry.telemetryLevel": "off",
    "markdown.preview.breaks": true,
    "workbench.statusBar.visible": true,
    "git.openRepositoryInParentFolders": "always",
    "[markdown]": {
        "editor.quickSuggestions": {
            "other": true,
            "comments": true,
            "strings": true,
        },
        "editor.fontSize": 14.5,
    },
    "markdown.preview.fontSize": 14.5,
    "breadcrumbs.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.stickyScroll.enabled": true,
    "editor.scrollbar.vertical": "visible",
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.minimap.showSlider": "always",
    "editor.scrollBeyondLastLine": true,
    "output.smartScroll.enabled": true,
    "markdown.styles": [
        ".vscode/css/styles.css"
    ],
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "workbench.colorCustomizations": {
